[user]
	name = sungjk
	email = ajax0615@gmail.com
[core]
    name = sungjk
    email = ajax0615@gmail.com
	excludesfile = /Users/jeremy/dotfiles/.gitignore_global
    precomposeunicode = true
    quotepath = false
[alias]
    c = commit -am
    co = checkout
    s = status
    undocommit = reset HEAD~
    hreset = reset --hard
    df = diff --color --color-words --abbrev
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --

[color]
    branch = auto
    diff = auto
    status = auto
    ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = "red bold 52"
	newNormal = "green bold"
	newHighlight = "green bold 22"
[color "diff"]
	meta = yellow bold
	commit = green bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
    newMoved = cyan
    oldMoved = blue
[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true

[fetch]
	prune = true
